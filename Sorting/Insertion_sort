#include<bits/stdc++.h>
using namespace std;
int main()
{
    cout<<"how many numbers:";
    int n,key,i,j;
    scanf("%d",&n);
    int A[n+2];
    cout<<"enter the numbers:";
    for(i=0;i<n;++i) scanf("%d",&A[i]);
    for(i=1;i<n;++i)
    {
        j=i-1;
        key=A[i];
        while(j>=0 and A[j]>key)
        {
            A[j+1]=A[j];
            A[j]=key;
            --j;
        }
    }
    cout<<"sorted values are:\n";
    for(i=0;i<n;++i) cout<<A[i]<<" ";
    return 0;
}
