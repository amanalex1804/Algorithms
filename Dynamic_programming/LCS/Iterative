#include<bits/stdc++.h>
using namespace std;
int main()
 {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
	int t;
	cin>>t;

	while(t--){
	    int n,m;cin>>n>>m;
	    string a,b;cin>>a>>b;
	    int dp[n+1][m+1];
	    
	    for(int i=0;i<=n;++i){
	        for(int j=0;j<=m;++j){
	            if(!i or !j) {dp[i][j]=0;continue;}
	            if(a[i-1]==b[j-1]) dp[i][j]=dp[i-1][j-1]+1;
	            else dp[i][j]=max(dp[i-1][j],dp[i][j-1]);
	        }
	    }
	    cout<<dp[n][m]<<"\n";
	
	}
	return 0;
}


// Time : O(n^2)
// Auxillary : O(n^2)
