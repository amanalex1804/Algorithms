#include<bits/stdc++.h>
using namespace std;
#define T int t;for(scanf("%d",&t);t--;)
#define rep(xx,uu,ee) for(xx=uu;xx<ee;++xx)
int main()
{
    int n,k,nsub,j,i,val;
    T{
    scanf("%d%d",&n,&k);
    int A[n+5];
    rep(i,0,n) scanf("%d",&A[i]);
    nsub=1<<n;
    for(i=0;i<nsub;++i)
    {
        cout<<"{";
        for(j=0;j<n;++j)
        {
            if((1<<j)&i)
                cout<<A[j]<<" ";

        }
        cout<<"}\n";
    }





    }


    return 0;
}
