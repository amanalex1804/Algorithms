#define SIZ 4000001
#define INF 999999
#define T int t;for(scanf("%d",&t);t--;)
#define mod 1000000007


/* double the first number and half the other number 
 a*b=  (2*a) * (b/2) 
*/
ull russianmult(ull a,ull b)
{
	ull res=0;
	while(b)
	{
		if(b&1) res+=a;
		a=a<<1;   //double the first
		b=b>>1;   // half the 
		
	}
	return res;
}
int main()
{
	
	cout<<russianmult(18,2);
	return 0;
}
